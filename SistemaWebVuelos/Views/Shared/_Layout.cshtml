<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/Content/main.css" rel="stylesheet" />
</head>
<body>

    <header>
        <section id="header">
            <h1>SISTEMA VUELOS</h1>
        </section>

    </header>

    <nav id="topmenu">
        <ul>
            <li>@Html.ActionLink("Home", "Index", "Home")</li>
            <li>@Html.ActionLink("Vuelos", "Index", "Vuelo")</li>
        </ul>
    </nav>

    <main>
        <aside id="izq">
            @if (IsSectionDefined("Izq"))
            {
                @RenderSection("Izq")
            }
            else
            {
                <img src="data:image/jpeg;base64," />
                <img src="https://ichef.bbci.co.uk/news/640/cpsprodpb/4792/production/_105322381_airbus_getty.jpg" />
            }
        </aside>
        <article>
            @RenderBody()
        </article>

    </main>
    <footer>Info@vuelo.com</footer>
</body>
</html>